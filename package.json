{
  "name": "aggregate-async-iterator",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "exports": {
    ".": "./src/aggregate-async-iterator.mjs"
  },
  "description": "Aggregates several async iterators into one (zip)",
  "keywords": [
    "aggregate",
    "asynciterator",
    "iterators",
    "sequence",
    "zip"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "test": "npm run test:ava",
    "test:ava": "ava --timeout 2m tests/*.mjs",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m tests/*.mjs && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme ./src/aggregate-async-iterator.mjs --section=API",
    "lint": "npm run lint:docs",
    "lint:docs": "documentation lint ./src/aggregate-async-iterator.mjs"
  },
  "devDependencies": {
    "ava": "^3.14.0",
    "c8": "^7.3.5",
    "documentation": "github:arlac77/documentation",
    "semantic-release": "^17.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/aggregate-async-iterator.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/aggregate-async-iterator/issues"
  },
  "homepage": "https://github.com/arlac77/aggregate-async-iterator#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-arlac77-github",
      "arlac77/template-esm-only"
    ]
  }
}
